<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.johnnie.mstorage">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".ImportData" />
        <activity android:name=".ViewData" />
        <activity android:name=".StartAudit" />
        <activity android:name=".MakeReport" />
        <activity android:name=".DepartmentsSelection" />
        <activity android:name=".DepartmentsSelectionInventory" />
        <activity android:name=".ItemsInventory" />
        <activity android:name=".ItemDetailsInventory"/>
        <activity android:name=".DepartmentAudit"/>
    </application>

</manifest>